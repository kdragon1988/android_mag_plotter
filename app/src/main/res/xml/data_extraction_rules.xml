<?xml version="1.0" encoding="utf-8"?>
<!--
    data_extraction_rules.xml
    
    Android 12以降のデータ抽出ルール
-->
<data-extraction-rules>
    <cloud-backup>
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
    </cloud-backup>
    <device-transfer>
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>



